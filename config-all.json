{
  "inbounds": [
    {
      "port": 443,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "VMESS_UUID",
            "alterId": 0,
            "security": "VMESS_SECURITY"
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "/vmess"
        }
      }
    },
    {
        "port": 443,
        "protocol": "vless",
        "settings": {
            "clients": [
                {
                    "id": "VMESS_UUID"
                }
            ],
            "decryption": "none"
        },
        "streamSettings": {
            "network": "ws",
            "wsSettings": {
                "path": "/vless"
            }
        }
    },
    {
        "port": 443,
        "protocol": "trojan",
        "settings": {
            "clients": [
                {
                    "password": "TROJAN_PWD"
                }
            ]
        },
        "streamSettings": {
            "network": "ws",
            "wsSettings": {
                "path": "/trojan"
            }
        }
    },
    {
        "port": 443,
        "protocol": "shadowsocks",
        "settings": {
            "method": "chacha20-ietf-poly1305",
            "password": "123456"
        },
        "streamSettings": {
            "network": "ws",
            "security": "tls",
            "wsSettings": {
                "path": "/shadowsocks"
            }
        }
    },
    {
        "listen": "127.0.0.1",
        "port": "1080",
        "protocol": "socks",
        "settings": {
            "auth": "noauth",
            "udp": true,
            "ip": "127.0.0.1"
        }
    }
  ],
    "outbounds": [
        {
            "protocol": "freedom",
            "tag": "direct",
            "settings": {}
        },
        {
            "protocol": "blackhole",
            "tag": "blocked",
            "settings": {}
        },
        {
            "protocol": "socks",
            "tag": "sockstor",
            "settings": {
                "servers": [
                    {
                        "address": "127.0.0.1",
                        "port": 9050
                    }
                ]
            }
        },
        {
            "protocol": "freedom",
            "tag": "twotag",
            "streamSettings": {
                "network": "domainsocket",
                "dsSettings": {
                    "path": "apath",
                    "abstract": true
                }
            }
        }
    ],
    
    "routing": {
        "rules": [
            {
                "type": "field",
                "inboundTag": [
                    "onetag"
                ],
                "outboundTag": "twotag"
            },
            {
                "type": "field",
                "outboundTag": "sockstor",
                "domain": [
                    "geosite:tor"
                ]
            },
            {
                "type": "field",
                "outboundTag": "blocked",
                "domain": [
                    "geosite:category-ads-all"
                ]
            }
        ]
    },
    "dns": {
        "servers": [
            "https://unfiltered.adguard-dns.com/dns-query",
            "https://dns.google/dns-query",
            "https://dns.cloudflare.com/dns-query"
        ]
    }
}
